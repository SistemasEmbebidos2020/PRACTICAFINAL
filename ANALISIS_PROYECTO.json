#include <Arduino.h>

// Define the structure to store project information
struct ProjectInfo {
  String fecha;
  String tipo_proyecto;
  int total_archivos;
  Hardware hardware[];
  Libreria librerias[];
  Pins pines;
};

void setup() {
  // Set up serial communication for debug purposes
  Serial.begin(115200);

  // Create a JSON object to store project information
  ProjectInfo proyecto;

  // Populate the project information
  proyecto.fecha = "2026-02-05T11:09:54.307591";
  proyecto.tipo_proyecto = "Embebido General";
  proyecto.total_archivos = 0;
  proyecto.hardware = {};
  proyecto.librerias = {};
  proyecto.pines = {};

  // Print the project information to the serial monitor
  Serial.println("Proyecto:");
  Serial.print("Fecha: ");
  Serial.println(proyecto.fecha);
  Serial.print("Tipo de Proyecto: ");
  Serial.println(proyecto.tipo_proyecto);
  Serial.print("Total Archivos: ");
  Serial.println(proyecto.total_archivos);

  // Print the hardware information to the serial monitor
  Serial.println("Hardware:");
  if (sizeof(proyecto.hardware) > 0) {
    for (int i = 0; i < sizeof(proyecto.hardware); i++) {
      Serial.print(" - Elemento: ");
      Serial.println(proyecto.hardware[i].nombre);
    }
  } else {
    Serial.println("No hardware assigned.");
  }

  // Print the libraries information to the serial monitor
  Serial.println("Librerias:");
  if (sizeof(proyecto.librerias) > 0) {
    for (int i = 0; i < sizeof(proyecto.librerias); i++) {
      Serial.print(" - Nombre: ");
      Serial.print(proyecto.librerias[i].nombre);
      Serial.print(", Version: ");
      Serial.println(proyecto.librerias[i].version);
    }
  } else {
    Serial.println("No libraries assigned.");
  }

  // Print the pins information to the serial monitor
  Serial.println("Pines:");
  if (sizeof(proyecto.pines) > 0) {
    for (int i = 0; i < sizeof(proyecto.pines); i++) {
      Serial.print(" - Nombre: ");
      Serial.print(proyecto.pines[i].nombre);
      Serial.print(", Tipo: ");
      Serial.println(proyecto.pines[i].tipo);
    }
  } else {
    Serial.println("No pins assigned.");
  }

}

